    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
