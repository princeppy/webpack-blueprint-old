{
  "name": "webpack-blueprint",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "npm run build -- --watch",
    "build": "webpack --mode production  --env production",
    "build-dev": "webpack --mode development  --env development",
    "build-server": "nodemon -w webpack.config.js -w config/babel.js -x http-server ./dist",
    "start": "nodemon -w webpack.config.js -w config/babel.js -x webpack-dev-server -- --config ./webpack.config.js --mode development  --env development --hot",
    "start-prod": "nodemon -w webpack.config.js -w config/babel.js -x webpack-dev-server -- --config ./webpack.config.js --mode production  --env production --hot"
  },
  "devDependencies": {
